# App Configuration
APP_NAME=PandoraX
APP_VERSION=1.0.0
DEBUG_MODE=true

# API Configuration
API_BASE_URL=https://api.pandorax.com
API_TIMEOUT=30000

# Encryption
ENCRYPTION_KEY=your_encryption_key_here

# Analytics
ANALYTICS_ENABLED=true
CRASH_REPORTING_ENABLED=true
